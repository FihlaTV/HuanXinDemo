<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/edt_user_name_main_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <EditText
        android:id="@+id/edt_main_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/btn_send"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Send"/>

    <Button
        android:id="@+id/btn_getmessage_history"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="gethistoryMessage"/>

    <Button
        android:id="@+id/btn_get_unread_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GetUnReadMessage"/>

    <Button
        android:id="@+id/btn_east_ui"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EasyUI"/>

</LinearLayout>